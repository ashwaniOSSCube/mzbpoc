<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
		<plugin>
			<pluginName>Elements_OutputDataConfigToolkit</pluginName>
			<pluginDescription><![CDATA[Elements_OutputDataConfigToolkit]]></pluginDescription>
            <!-- className of the plugin which extends Pimcore_API_Plugin_Abstract-->
			<pluginClassName>\Elements\OutputDataConfigToolkit\Plugin</pluginClassName>
			<!-- include paths relative to plugin-directory -->
			<pluginIncludePaths>
				<path>/Elements_OutputDataConfigToolkit/lib</path>
				<path>/Elements_OutputDataConfigToolkit/controllers</path>
                <path>/Elements_OutputDataConfigToolkit/models</path>
			</pluginIncludePaths>
			<!-- namespaces to register with autoloader-->
			<pluginNamespaces>
				<namespace>OutputDataConfigToolkit</namespace>
			</pluginNamespaces>
			<!-- js files with path relative to plugin-directory -->
			<pluginJsPaths>
                <path>/Elements_OutputDataConfigToolkit/static/js/Plugin.js</path>
                <path>/Elements_OutputDataConfigToolkit/static/js/OutputDataConfigTab.js</path>
                <path>/Elements_OutputDataConfigToolkit/static/js/OutputDataConfigDialog.js</path>


                <path>/Elements_OutputDataConfigToolkit/static/js/outputDataConfigElements/Abstract.js</path>
                <path>/Elements_OutputDataConfigToolkit/static/js/outputDataConfigElements/value/DefaultValue.js</path>
