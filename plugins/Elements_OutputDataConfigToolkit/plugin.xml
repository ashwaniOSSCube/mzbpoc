                <path>/Elements_OutputDataConfigToolkit/static/js/outputDataConfigElements/value/KeyValue.js</path>
                <path>/Elements_OutputDataConfigToolkit/static/js/outputDataConfigElements/value/Numberic.js</path>

                <path>/Elements_OutputDataConfigToolkit/static/js/outputDataConfigElements/operator/Text.js</path>
                <path>/Elements_OutputDataConfigToolkit/static/js/outputDataConfigElements/operator/Group.js</path>
                <path>/Elements_OutputDataConfigToolkit/static/js/outputDataConfigElements/operator/Concatenator.js</path>
