                <path>/Elements_OutputDataConfigToolkit/static/js/outputDataConfigElements/operator/TableRow.js</path>
                <path>/Elements_OutputDataConfigToolkit/static/js/outputDataConfigElements/operator/TableCol.js</path>
			</pluginJsPaths>
			<!-- css files with path relative to plugin-directory -->
			<pluginCssPaths>
                <path>/Elements_OutputDataConfigToolkit/static/css/admin.css</path>
			</pluginCssPaths> 
		</plugin>
</zend-config>

